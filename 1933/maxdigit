public class Max{
  public static void main(String[] args) {}


  public static int recursiveMaxDigit(int num){}
    if num < 10
      return helperMaxDigit(num)
    elif num % 10 > helperMaxDigit()
      helperMaxDigit(num)
      return recursiveMaxDigit(num/10)
    else
      return recursiveMaxDigit(num/10)


  public static int helperMaxDigit(int num){
    int max = num
    return max
  }
  public static int iterativeMaxDigit(int num){}
}
